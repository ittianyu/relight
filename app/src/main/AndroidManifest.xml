<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ittianyu.relight">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".base.BaseTutorialActivity" />
        <activity android:name=".base._1.WidgetActivity" />
        <activity android:name=".base._2.WidgetActivity" />
        <activity android:name=".base._3.WidgetActivity" />
        <activity android:name=".base._4.WidgetActivity" />
        <activity android:name=".base._5.WidgetActivity" />
        <activity android:name=".base._6.WidgetActivity" />
        <activity android:name=".base._7.WidgetActivity" />
        <activity android:name=".medium.MediumTutorialActivity" />
        <activity android:name=".medium._1.WidgetActivity" />
        <activity android:name=".medium._2.WidgetActivity" />
        <activity android:name=".medium._3.WidgetActivity" />
        <activity android:name=".medium._4.WidgetActivity" />
        <activity android:name=".medium._5.WidgetActivity"/>
        <activity android:name=".medium._6.WidgetActivity" />
        <activity android:name=".medium._7.WidgetActivity"/>
        <activity android:name=".medium._8.WidgetActivity" />

    </application>

</manifest>